{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Flutter: generate assets",
      "group": "build",
      "type": "shell",
      "command": "fluttergen"
    },
    {
      "label": "Flutter: build runner",
      "group": "build",
      "type": "shell",
      "command": "flutter",
      "args": [
        "pub",
        "run",
        "build_runner",
        "build",
        "--delete-conflicting-outputs"
      ]
    }
  ]
}
